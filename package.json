    "firebase-admin": "^7.3.0",
    "helmet": "3.16.0",
    "jsonwebtoken": "8.5.1",
    "lexing": "^0.8.2",
    "moment": "2.24.0",
    "morgan": "^1.9.1",
    "multer": "1.4.1",
    "request-promise": "4.2.4",
